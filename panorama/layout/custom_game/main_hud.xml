                        <Image id="CardPrice" src="file://{resources}/images/custom_game/Regeneration_Rune_minimap_icon.png"/>
                    </Panel>
                    
                    
                    
                    <!-- <Label id="CardPrice" text = "ccccasdddddddcc" /> -->
                </Panel>
            </Button>
        </snippet>
    </snippets>
    
    
    
    
