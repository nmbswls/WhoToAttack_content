                        <Label id="CardName" class="TitleFont" text=""/>
                    </Panel>
                    <Image id="CardImage" src="file://{resources}/images/custom_game/img_brawn_siege.png"/>
                    
                    <Panel id="CardBottomBar">
                        <Label id="CardDesp" class="TitleFont" text = "恶魔/1*" />
